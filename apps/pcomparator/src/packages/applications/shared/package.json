{
  "name": "@deazl/shared",
  "version": "0.0.1",
  "description": "Shared components for Deazl applications",
  "keywords": [
    "deazl",
    "shared",
    "application",
    "react"
  ],
  "author": "Cl√©ment Muth <clementmuth@deazl.fr>",
  "homepage": "https://deazl.fr",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Deazl-Comparator/deazl.git",
    "directory": "pcomparator/packages/applications/shared"
  },
  "bugs": {
    "url": "https://github.com/Deazl-Comparator/deazl/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-rc.0",
    "react-dom": ">=18 || >=19.0.0-rc.0"
  },
  "clean-package": "../../../../clean-package.config.json",
  "dependencies": {
    "@deazl/components": "workspace:^",
    "@deazl/system": "workspace:^"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "tsup": "^8.5.0"
  }
}
